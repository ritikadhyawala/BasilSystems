<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ButtonView">
        <attr name="circleColor" format="color" />
        <attr name="circleLabel" format="string"></attr>
        <attr name="offColor" format="color"></attr>
        <attr name="onColor" format="color"></attr>
        <attr name="autoColor" format="color"></attr>
        <attr name="normalColor" format="color"></attr>
        <attr name="isAuto" format="boolean"></attr>
        <attr name="isChecked" format="boolean"></attr>
    </declare-styleable>
    <declare-styleable name="WeekButton">
        <attr name="unselectedColor" format="color"></attr>
        <attr name="weekLabel" format="string"></attr>
        <attr name="selectedColor" format="color"></attr>
        <attr name="weekLabelColor" format="color"></attr>
        <attr name="weekButtonChecked" format="boolean"></attr>
    </declare-styleable>
    <declare-styleable name="RadioButton">
        <attr name="radioButtonChecked" format="boolean"></attr>
        <attr name="outerGreenColor" format="color"></attr>
    </declare-styleable>

</resources>